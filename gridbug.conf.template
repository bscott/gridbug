[GRIDBUG]
DEBUG = no
ID = ZZ_ID
NODEURL = ZZ_URL
ROLE = node
CONSOLE = gridbug.html
SERVERNODE = ZZ_URL
GRIDKEY = ZZ_KEY
IPSERVICE = https://api.ipify.org

[API]
# Port for API requests
ENABLE = yes
PORT = ZZ_PORT
MAXPAYLOAD = 10000

[BUGS]
# Gridbug node settings
POLL = 10
TTL = 60
TIMEOUT = 10

[ALERT]
# Notify connectivity issues
ENABLE = yes
# TODO add ability to alert
